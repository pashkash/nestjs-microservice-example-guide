{
  "name": "@custom/microservice-template",
  "version": "0.2.1",
  "description": "Microservice Template",
  "author": "--",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start main",
    "start:dev": "yarn run start --watch",
    "start:debug": "yarn run start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn run test:unit",
    "test:unit": "jest --config ./jest.unit.json --coverage",
    "test:unit:watch": "yarn run test:unit --watch",
    "test:integration": "docker-compose up",
    "console": "node dist/console",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "generate:dto:svg": "tplant -i src/modules/sample-processor/dto/index.ts -o public/index.dto.svg -C",
    "generate:open-api-schema": "yarn run --silent console generate open-api-schema > public/openapi-schema.json",
    "generate:redoc": "redoc-cli bundle -o public/redoc.html public/openapi-schema.json",
    "changelog": "auto-changelog -p",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/config/postgres.config.ts"
  },
  "dependencies": {
    "@custom/nestjs-config": "^1.5.0",
    "@custom/nestjs-logger": "^1.5.0",
    "@custom/cache-service-client": "^1.2.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/microservices": "^7.4.2",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/platform-fastify": "latest",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.0",
    "@types/locutus": "^0.0.6",
    "auto-changelog": "^2.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "commander": "^6.0.0",
    "compression": "^1.7.4",
    "console": "^0.7.2",
    "deep-equal": "^2.0.3",
    "fastify-swagger": "^2.6.0",
    "helmet": "^3.23.3",
    "ioredis": "^4.17.3",
    "locutus": "^2.0.11",
    "mockdate": "^3.0.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.18.1",
    "nest-winston": "^1.3.6",
    "nestjs-console": "^3.0.6",
    "node-vault": "^0.9.18",
    "node-vault-client": "^0.5.6",
    "os": "^0.1.1",
    "pg": "^8.3.0",
    "pjson": "^1.0.9",
    "postgres": "^1.0.2",
    "redis": "^3.0.2",
    "redoc": "2.0.0-rc.31",
    "redoc-cli": "^0.9.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "sentry-transport-winston": "^1.0.2",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "tplant": "^2.3.3",
    "typeorm": "^0.2.25",
    "uuid": "^8.2.0",
    "uuid-generator-ts": "^1.0.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.3",
    "@types/compression": "^1.7.0",
    "@types/crypto-js": "^3.1.47",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.4",
    "@types/helmet": "0.0.47",
    "@types/jest": "26.0.5",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.3",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack-node-externals": "^2.5.0"
  },
  "config": {
    "swagger": {
      "title": "Microservice Template",
      "description": "Use it to start making Microservice"
    }
  },
  "bin": {
    "microservice-template": "./dist/src/main.js",
    "microservice-template-cli": "./dist/src/console.js"
  },
  "homepage": "https://github.com/pashkash/nestjs-microservice-example-guide",
  "bugs": "https://github.com/pashkash/nestjs-microservice-example-guide/issues",
  "repository": {
    "type": "https",
    "url": "https://github.com/pashkash/nestjs-microservice-example-guide.git"
  },
  "auto-changelog": {
    "mergeUrl": "https://github.com/pashkash/nestjs-microservice-example-guide/merge_requests/{id}",
    "template": "keepachangelog"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint && yarn run test && yarn run format"
    }
  }
}
