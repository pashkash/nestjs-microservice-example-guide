{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": [".dto.ts", ".entity.ts", "Dto.ts"],
          "controllerFileNameSuffix": [".controller.ts", "Controller.ts"]
        }
      }
    ]
  }
}
